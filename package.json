{
  "name": "check-i18n",
  "version": "1.0.10",
  "description": "Check (Angular) JSON i18n translations from link in local directory.",
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkLeopard/check-i18n"
  },
  "bugs": {
    "url": "https://github.com/DarkLeopard/check-i18n/issues"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run build",
    "test:start": "npm run build && npm link",
    "test:js": "npm run test:start && node ./dist/index.js --i18nDir=./tests/mocks/example.json --findDir=./tests/mocks/find-dir --cutKeys=true --replacePlural=true",
    "test:sh": "npm run test:start && check-i18n -f=./tests/mocks/example.json -d=./tests/mocks/find-dir --cutkeys=true --replaceplural=true"
  },
  "bin": {
    "check-i18n": "./cli.sh"
  },
  "eslintIgnore": [
    "build",
    "node_modules",
    "reports"
  ],
  "dependencies": {
    "ansi-colors": "4.1.3",
    "cli-progress": "3.11.1",
    "lodash": "4.17.21",
    "rxjs": "7.5.4",
    "ts-node": "10.8.0"
  },
  "devDependencies": {
    "@types/cli-progress": "3.11.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-unicorn": "22.0.0",
    "rimraf": "^3.0.2",
    "rxjs-tslint": "0.1.8",
    "typescript": "4.7.2"
  },
  "author": "Barabanov Sergey Konstantinovich",
  "license": "Apache-2.0",
  "keywords": [
    "i18n",
    "translate",
    "traslations",
    "angular",
    "ngx-translate"
  ]
}
